++
++	for (i = 1; fc_weight > map[i].fc; i++)
++	  ;
++
++	if (fc_weight == map[i].fc)
++	  return map[i].ot;
++
++	/* Interpolate between two items. */
++	return lerp (fc_weight, map[i-1].fc, map[i].fc, map[i-1].ot, map[i].ot);
++}
